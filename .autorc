{
  "plugins": [
    "npm",
    "released",
    "first-time-contributor"
  ],
  "owner": "UI-Components-Books-Books",
  "repo": "UI-Components-books-TS",
  "name": "Frontend-leader",
  "email": "coor_frontend@booksandbooksdigital.com.co",
  "labels": [
    {
      "name": "major",
      "changelogTitle": "ğŸ’¥ Breaking Change",
      "description": "Increment the major version when merged",
      "releaseType": "major"
    },
    {
      "name": "minor",
      "changelogTitle": "ğŸš€ Enhancement",
      "description": "Increment the minor version when merged",
      "releaseType": "minor"
    },
    {
      "name": "patch",
      "changelogTitle": "ğŸ› Bug Fix",
      "description": "Increment the patch version when merged",
      "releaseType": "patch"
    },
    {
      "name": "skip-release",
      "description": "Preserve the current version when merged",
      "releaseType": "skip"
    },
    {
      "name": "release",
      "description": "Create a release when this pr is merged",
      "releaseType": "release"
    },
    {
      "name": "internal",
      "changelogTitle": "ğŸ  Internal",
      "description": "Changes only affect the internal API",
      "releaseType": "none"
    },
    {
      "name": "documentation",
      "changelogTitle": "ğŸ“ Documentation",
      "description": "Changes only affect the documentation",
      "releaseType": "none"
    },
    {
      "name": "tests",
      "changelogTitle": "ğŸ§ª Tests",
      "description": "Add or improve existing tests",
      "releaseType": "none"
    },
    {
      "name": "dependencies",
      "changelogTitle": "ğŸ”© Dependency Updates",
      "description": "Update one or more dependencies version",
      "releaseType": "patch"
    },
    {
      "name": "performance",
      "changelogTitle": "ğŸ Performance",
      "description": "Improve performance of an existing feature",
      "releaseType": "patch"
    }
  ],
  "shipit": {
    "noChangelog": false,
    "message": "Release: %v [skip ci]"
  },
  "baseBranch": "main"
}
